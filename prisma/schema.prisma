generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

// sample schema
model Testing {
  id           String @id @default(uuid())
  testName     String
  descriptions String

  @@map("testings")
}
